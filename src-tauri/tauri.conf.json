{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "nmap3d",
  "version": "0.1.0",
  "identifier": "com.nmap3d.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "transparent": false,
        "decorations": true,
        "fullscreen": false,
        "resizable": true,
        "title": "App",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
